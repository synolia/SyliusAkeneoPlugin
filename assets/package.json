  "repository": "git@bitbucket.org:synolia/sylius-akeneo-plugin.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "parcel build src/index.js src/js/form.js src/js/tooltip.js --out-dir ../src/Resources/public --out-file main.js --no-source-maps",
